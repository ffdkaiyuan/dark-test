{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/assign-code.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/assign-code",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/api/assign-code.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
